---
import Image from "astro/components/Image.astro";

import Logo from "../../assets/images/Grant/price-197.png";
---

<div
  class="container"
  style="padding-top: 5px; padding-bottom: 5px; outline: none;"
>
  <div class="containerInner ui-sortable">
    <div
      style="padding-top: 5px; padding-bottom: 5px; margin: 0px; outline: none;"
    >
      <div style="outline: none;">
        <div style="padding: 0 10px">
          <div
            class="text-center"
            style="margin-top: 20px; outline: none; display: block; cursor: pointer;"
          >
            <Image
              src={Logo}
              alt="Price Cut"
              class="price_cut static_fit"
              width="850"
            />
          </div>
          <div
            style="margin-top: 0px; outline: none; cursor: pointer; display: block;"
          >
            <div
              class="font_22"
              style="text-align: center; font-size: 28px; color: rgb(0, 68, 130);"
            >
              <b>HURRY!</b> This Is Only Available For The Next 5 Minutes...<br
              />
            </div>
          </div>
          <div
            class="de elCountdownEvergreen elAlign_center elMargin0 ui-droppable de-editable text-center"
            id="tmp_countdown_evergreen-75640"
            style="margin-top: 15px; outline: none; display: block; cursor: pointer;"
          >
            <div
              class="wideCountdownEvergreen clearfix cdLabelBold cdYellow cdStyleCircleFill wideCountdown-demo is-countdown wideCountdownSize4"
            >
              <span class="countdown-row countdown-show3"
                ><span class="countdown-section"
                  ><span class="countdown-section"
                    ><span class="countdown-amount">00</span><span
                      class="countdown-period">Hours</span
                    ></span
                  ><span class="countdown-section"
                    ><span class="countdown-amount">00</span><span
                      class="countdown-period">Minutes</span
                    ></span
                  ><span class="countdown-section"
                    ><span class="countdown-amount">00</span><span
                      class="countdown-period">Seconds</span
                    ></span
                  ></span
                ></span
              >
            </div>
          </div>
        </div>
        <div
          style="outline: none; cursor: pointer; display: block; margin-top: 25px; text-align: center;"
        >
          <a
            href="https://gc.grantcardone.com/playbook-so?declined=true&errors=Sorry%20Your%20Cart%20Session%20has%20expired#yes-link"
            class="elButton elButtonColor1 trk1 elButtonSize4 elButtonIcon8 elButtonSubtle elButtonFluid mfs_18 font_18"
            style="color: rgb(255, 255, 255); background-color: rgb(228, 59, 44); font-size: 32px;"
          >
            <span class="elButtonMain"
              >Yes! Add The Playbook To Millions To My Order!</span
            >
            <span class="elButtonSub"
              >Clicking On This Button Will Charge Your Card $197 Now</span
            >
          </a>
        </div>
        <div
          style="margin-top: 5px; outline: none; cursor: pointer; display: block;"
        >
          <div
            class="ne elHeadline hsSize2 lh3 elMargin0 elBGStyle0 hsTextShadow0 mfs_22"
            style="text-align: center; font-size: 26px; color: rgb(0, 0, 0);"
            data-bold="inherit"
            contenteditable="false"
          >
            <u style="color: rgb(0, 0, 0);"
              ><a
                href="https://gc.grantcardone.com/playbook-so?upsell=0&purchase%5Bproduct_id%5D=2537282#no-link"
                id="link-3994-441-796"
                class="font_18"
                target="_self"
                style="color: rgb(0, 0, 0);"
                >No, I'm Not Interested in This Amazing Offer</a
              ></u
            >
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

